package Encapsulation;

public class ATM {
	
	private int pin ;
	private int otp ;
	private String name; 
	
	public void setpin(int outpin) {
		pin = outpin;
	}
	public int getpin() {
		return pin;
	}
	
	public static void main(String[] args) {
		ATM obj = new ATM();
		obj.setpin(4543);
		
		System.out.println("The ATM pin is : "+obj.getpin());
	}
	

}
