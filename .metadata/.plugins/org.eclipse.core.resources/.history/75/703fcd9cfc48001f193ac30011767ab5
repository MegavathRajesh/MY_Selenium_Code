package javaPractice;

public class Ifelse {

	public static void main(String[] args) {
		 int a = 10 ;
		 
		 if(a>20) {
			 
			 System.out.println("the condition will true");
		 }
		 
		 else if (a==10) {
			 
			 System.out.println("The Condition will equal ");
		 }
		 else {
			 
			 System.out.println("the condition will false");
		 }

	}

}
