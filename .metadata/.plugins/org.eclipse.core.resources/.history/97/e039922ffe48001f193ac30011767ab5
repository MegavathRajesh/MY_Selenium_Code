package javaPractice;

public class Switchclass {

	public static void main(String[] args) {
	 
		int a = 20;
		int b=  2;
		
		switch (2) {
		
		case 1 :
			System.out.println("The Value of A and B = "+(a+b));
		  break;
		
			
		case 2 :
			System.out.println("The Value of A and B = "+(a-b));
			break;
			
		case 3 :
			System.out.println("The Value of A and B = "+a/b);
			break;
			
		case 4 :
			System.out.println("The Value of A and B = "+a*b);
			break;
		}
		
	}

}
