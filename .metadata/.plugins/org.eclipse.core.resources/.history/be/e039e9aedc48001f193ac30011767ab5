package variables;

public class Staticvariable {
	
	static String sr = "Hai This Static variable";
	static String tr = "iam Static I don't need to create a object ";
	static  String rd = "Iam Accecpted With Static Keyword";
	

	public static void main(String[] args) {
		
		System.out.println(sr);
		System.out.println(tr);
		System.out.println(rd);
		 
		
	}

}
